script/source = "extends Node2D

# Game state variables
var gold = 300
var lives = 10
var max_lives = 10
var current_wave = 0
var max_waves = 20
var game_state = \"menu\"
var total_kills = 0
var enemies_alive = 0
var enemies_per_wave = 0

# Difficulty scaling factors
var health_scaling = 1.0
var speed_scaling = 1.0
var spawn_rate_scaling = 1.0

# References
@onready var ui = $UI
@onready var life_point = $LifePoint
@onready var path_spawner = $PathSpawner
@onready var towers_container = $Towers

# In your main game script
var current_tower_to_place = null
var current_build_zone = null

func _ready():
	# Initial setup
	update_ui()
	
	# Connect signals
	life_point.body_entered.connect(_on_life_point_body_entered)
	
	# Start in menu state
	set_game_state(\"menu\")
	# Test if groups are working
	var all_nodes = get_tree().get_nodes_in_group(\"build_zones\")
	print(\"Total nodes in 'build_zones' group:\", all_nodes.size())
	for node in all_nodes:
		print(\"  - \", node.name, \" (Type: \", node.get_class(), \")\")

func set_game_state(state):
	game_state = state
	
	match state:
		\"menu\":
			ui.show_menu()
			path_spawner.stop_spawning()
			# Reset difficulty scaling when back to menu
			health_scaling = 1.0
			speed_scaling = 1.0
			spawn_rate_scaling = 1.0
		\"playing\":
			ui.show_game_ui()
			start_next_wave()
		\"gameover\":
			ui.show_game_over(total_kills, current_wave, calculate_stars(current_wave))
			path_spawner.stop_spawning()
		\"victory\":
			ui.show_victory(total_kills, current_wave, 3)

func start_next_wave():
	if game_state != \"playing\":
		return
	
	current_wave += 1
	
	if current_wave > max_waves:
		set_game_state(\"victory\")
		return
	
	# Calculate wave difficulty with progressive scaling
	enemies_per_wave = calculate_enemies_in_wave(current_wave)
	var enemy_health = calculate_enemy_health(current_wave)
	var enemy_speed = calculate_enemy_speed(current_wave)
	var spawn_rate = calculate_spawn_rate(current_wave)
	
	# Reset alive counter
	enemies_alive = 0
	
	# Update wave UI
	ui.update_waves(current_wave, max_waves)
	
	# Update wave message
	ui.update_wave_message(current_wave, enemy_health, enemy_speed)
	
	# Start spawning enemies with difficulty settings
	path_spawner.start_wave(current_wave, enemies_per_wave, enemy_health, enemy_speed, spawn_rate)
	
	print(\"Wave \", current_wave, \": \", enemies_per_wave, \" enemies\")
	print(\"Health: \", enemy_health, \" | Speed: \", enemy_speed, \" | Spawn Rate: \", spawn_rate)

func calculate_enemies_in_wave(wave):
	# Progressive enemy count increase
	# Base formula: 5 + (wave * 2) for waves 1-10, then 15 + (wave * 3) for waves 11-20
	if wave <= 10:
		return 5 + (wave * 2)  # 7 to 25 enemies
	else:
		return 15 + ((wave - 10) * 3)  # 18 to 45 enemies

func calculate_enemy_health(wave):
	# Exponential health increase
	# Wave 1: 10 health, Wave 10: ~25 health, Wave 20: ~60 health
	var base_health = 10
	var scaling_factor = 1.0 + (wave * 0.08)  # 8% increase per wave
	return round(base_health * scaling_factor)

func calculate_enemy_speed(wave):
	# Speed increases every 5 waves
	var base_speed = 20
	if wave <= 5:
		return base_speed
	elif wave <= 10:
		return base_speed * 1.5  # 50% faster
	elif wave <= 15:
		return base_speed * 2.0  # 100% faster
	else:
		return base_speed * 2.5  # 150% faster

func calculate_spawn_rate(wave):
	# Spawn rate increases (lower interval = faster spawns)
	# Wave 1: 2.0 seconds between spawns
	# Wave 20: 0.3 seconds between spawns (very fast!)
	var base_rate = 2.0
	var min_rate = 0.3
	var rate = base_rate - (wave * 0.085)  # Decrease by 0.085 seconds per wave
	return max(rate, min_rate)  # Don't go below min_rate

func calculate_stars(waves_survived):
	if waves_survived >= max_waves:
		return 3
	elif waves_survived >= 15:
		return 2
	elif waves_survived >= 10:
		return 1
	else:
		return 0

func enemy_spawned():
	enemies_alive += 1

func enemy_removed():
	enemies_alive -= 1
	if enemies_alive < 0:
		enemies_alive = 0
	
	# Check if wave is complete
	if game_state == \"playing\" and enemies_alive == 0:
		if path_spawner.is_wave_complete():
			# Wait before next wave (shorter wait for higher waves)
			var wait_time = max(1.0, 3.0 - (current_wave * 0.1))  # From 3s to 1s
			await get_tree().create_timer(wait_time).timeout
			if game_state == \"playing\":
				start_next_wave()

func enemy_killed():
	total_kills += 1
	# Increase gold reward based on wave difficulty
	var gold_reward = 25 + (current_wave * 2)  # More gold for harder waves
	add_gold(gold_reward)
	enemy_removed()

func update_ui():
	if ui and ui.has_method(\"update_gold\"):
		ui.update_gold(gold)
	if ui and ui.has_method(\"update_lives\"):
		ui.update_lives(lives)

func subtract_life(amount = 1):
	lives -= amount
	if lives < 0:
		lives = 0
	
	update_ui()
	
	if lives <= 0:
		set_game_state(\"gameover\")

func add_gold(amount):
	gold += amount
	update_ui()

func subtract_gold(amount):
	if gold >= amount:
		gold -= amount
		update_ui()
		return true
	return false

func _on_life_point_body_entered(body):
	if body.is_in_group(\"enemy\"):
		subtract_life()
		enemy_removed()
		body.queue_free()

func _on_start_button_pressed():
	if game_state == \"menu\":
		set_game_state(\"playing\")
	elif game_state == \"gameover\" or game_state == \"victory\":
		restart_game()

func restart_game():
	gold = 300
	lives = 10
	current_wave = 0
	total_kills = 0
	enemies_alive = 0
	game_state = \"menu\"
	health_scaling = 1.0
	speed_scaling = 1.0
	spawn_rate_scaling = 1.0
	
	clear_enemies()
	clear_towers()
	
	update_ui()
	set_game_state(\"menu\")

func clear_enemies():
	for node in get_tree().get_nodes_in_group(\"enemy\"):
		node.queue_free()

func clear_towers():
	# Clear all towers
	for child in towers_container.get_children():
		child.queue_free()
	
	# Reset all build zones to unoccupied
	reset_build_zones()

func reset_build_zones():
	var build_zones = get_tree().get_nodes_in_group(\"build_zones\")
	for zone in build_zones:
		zone.set_meta(\"occupied\", false)
		if zone.has_node(\"Sprite2D\"):
			zone.get_node(\"Sprite2D\").modulate = Color(1, 1, 1, 0.3)
			
func _input(event):
	if event is InputEventMouseButton and event.pressed:
		if event.button_index == MOUSE_BUTTON_LEFT:
			if current_tower_to_place != null:				# Try to place tower in a build zone
				place_tower_in_zone()
			else:
				# Maybe select a tower from UI
				pass
		if event.button_index == MOUSE_BUTTON_LEFT and event.button_mask == 0:
			# Add debug build zone where clicked
			var mouse_pos = get_global_mouse_position()
			print(\"Click at:\", mouse_pos)
			add_debug_build_zone(mouse_pos)

func add_debug_build_zone(position: Vector2):
	var zone = Area2D.new()
	zone.name = \"DebugZone\"
	zone.global_position = position
	
	var sprite = Sprite2D.new()
	sprite.texture = preload(\"res://icon.svg\")  # Use any texture
	sprite.modulate = Color(0, 1, 0, 0.5)
	zone.add_child(sprite)
	
	var collision = CollisionShape2D.new()
	var shape = RectangleShape2D.new()
	shape.extents = Vector2(25, 25)
	collision.shape = shape
	zone.add_child(collision)
	
	zone.add_to_group(\"build_zones\")
	zone.set_script(preload(\"res://scripts/build_zone.gd\"))
	
	$BuildZones.add_child(zone)
	print(\"Added debug build zone at:\", position)

func place_tower_in_zone():
	if current_build_zone and not current_build_zone.is_occupied:
		var tower = current_build_zone.place_tower(current_tower_to_place)
		if tower:
			current_tower_to_place = null
			current_build_zone = null
